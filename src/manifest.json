{
  "name": "douban",
  "version": "1.0.2",
  "description": "The extension for douban movie.",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://movie.douban.com/subject/*"],
      "js": ["douban.js"],
      "run_at": "document_end"  
    },
    {
      "matches": ["https://xinghe.tv/play/*"],
      "js": ["watch.js"],
      "run_at": "document_end"
    }
  ]
}